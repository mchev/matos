APP_NAME=matos.live
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Branding (optional overrides)
# APP_SLOGAN="Le matériel de vos événements, en un clic"
# APP_DESCRIPTION="Location et prêt de matériel entre associations et particuliers. Partagez, louez, facilitez vos événements."
# APP_CONTACT_EMAIL=contact@matos.live

APP_LOCALE=fr
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=fr_FR

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=matos_live
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=1440
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=redis

CACHE_STORE=redis
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

OVH_ACCESS_KEY_ID=
OVH_SECRET_ACCESS_KEY=
OVH_DEFAULT_REGION=bhs
OVH_URL=https://matos-live.s3.bhs.io.cloud.ovh.net/
OVH_ENDPOINT=https://s3.bhs.io.cloud.ovh.net/
OVH_BUCKET=matos-live
OVH_USE_PATH_STYLE_ENDPOINT=true

VITE_APP_NAME="${APP_NAME}"

# Inertia SSR. Default port 13714. For multiple sites on same server (e.g. Forge), use a unique port per site:
# - Site 1: leave defaults (13714) or set INERTIA_SSR_URL=http://127.0.0.1:13714
# - Site 2: set INERTIA_SSR_URL=http://127.0.0.1:13715 and in Supervisor add environment=INERTIA_SSR_PORT=13715
# INERTIA_SSR_URL=http://127.0.0.1:13714
